{
  "name": "Keyword Redirect GoTo",
  "description": "Shortcuts to your long url's. Type 'goto' and a keyword into the address bar to open url.",
  "version": "1.0",
  "options_page": "src/options/options.html",
  "background": {
    "page": "src/background/background.html"
  },
  "permissions": ["storage", "tabs"],
  "omnibox": { "keyword": "goto/" },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "16": "assets/extension-icons/icon16.png",
      "32": "assets/extension-icons/icon32.png"
    }
  },
  "icons": {
    "16": "assets/extension-icons/icon16.png",
    "32": "assets/extension-icons/icon32.png",
    "48": "assets/extension-icons/icon48.png",
    "128": "assets/extension-icons/icon.png"
  }
}
